{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "phoenix-beta": [
        "dist/phoenix-beta"
      ],
      "phoenix-beta/*": [
        "dist/phoenix-beta/*"
      ],
      "phoenix-prod-sa": [
        "dist/phoenix-prod-sa"
      ],
      "phoenix-prod-sa/*": [
        "dist/phoenix-prod-sa/*"
      ],
      "phoenix-beta-sa": [
        "dist/phoenix-beta-sa"
      ],
      "phoenix-beta-sa/*": [
        "dist/phoenix-beta-sa/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}